    "@types/morgan": "1.9.0",
    "@types/redis": "2.8.17",
    "@types/socket.io": "2.1.4",
    "@types/supertest": "2.0.8",
    "jest": "25.3.0",
    "jest-runner-groups": "1.1.1",
    "supertest": "4.0.2",
    "ts-jest": "25.4.0",
    "typescript": "3.8.3"
